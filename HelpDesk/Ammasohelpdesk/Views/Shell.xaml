<Window x:Class="Ammasohelpdesk.Views.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:viewmodels="clr-namespace:Ammasohelpdesk.ViewModels"
        Title="{Binding Title}"
        Height="300"
        Width="300">
    <Window.InputBindings>
        <KeyBinding Key="Escape" Command="{Binding Exit}" />


    </Window.InputBindings>
    <Window.DataContext>
        <viewmodels:ShellViewModel />

    </Window.DataContext>
    <Grid>


    </Grid>
</Window>
